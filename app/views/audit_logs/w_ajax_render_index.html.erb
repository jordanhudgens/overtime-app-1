
<%#= @audit_logs.inspect %>

<%# @audit_logs.each do |log| %>
	<%#= log.start_date %>
	<%#= log.end_date %>
	<%#= log.status %>
<%# end %>

<div class="container">
  <h2>Audit Log Dashboard</h2>
  <table class="table table-hover">
    <thead>
      <tr>
        <th>#</th>
        <th>Employee</th>
        <th>Week Starting</th> <!-- start date -->
        <th>Confirmed at</th>  <!-- end date -->
        <th>status</th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody id="audit_logs">

      <%= render @audit_logs %>

    </tbody>

  </table>
</div>

<div id="paginator">
  <%= paginate @audit_logs, remote: true %>
  <%#= paginate @paudit_logs %>
</div>





 
